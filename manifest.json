{
  "name": "Jumping Tabs",
  "version": "1.0",
  "description": "Quickly move tabs between windows.",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Jumping Tabs",
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  }
}
