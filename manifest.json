{
  "manifest_version": 2,
  "name": "Jumping Tabs",
  "version": "1.0",
  "description": "Quickly move tabs between windows.",
  "browser_action": {
    "default_title": "Jumping Tabs",
    "default_icon": "icon.png"
  },
  "author": "Jian Yuan Lee",
  "background": {
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "homepage_url": "https://github.com/jianyuan/chrome-jumpingtabs",
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
